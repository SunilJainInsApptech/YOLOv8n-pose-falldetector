{
  "module_id": "viam-labs:YOLOv8",
  "visibility": "public",
  "url": "https://github.com/viam-labs/YOLOv8",
  "description": "A Viam modular computer vision service for YOLOv8 inference, including models on HuggingFace",
  "models": [
    {
      "api": "rdk:service:vision",
      "model": "viam-labs:vision:yolov8",
      "markdown_link": "README.md#configure-your-vision-service",
      "short_description": "Supports YOLOv8 inference, including models on HuggingFace"
    }
  ],
  "build": {
    "path": "module.tar.gz",
    "arch": [
      "linux/arm64",
      "linux/amd64"
    ]
  },
  "entrypoint": "./run.sh",
  "first_run": "./setup.sh"
}
